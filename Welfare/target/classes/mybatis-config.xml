		<typeAlias type="com.greenart.bdproject.dto.NoticeDto" alias="NoticeDto"/>
		<typeAlias type="com.greenart.bdproject.dto.FaqDto" alias="FaqDto"/>
		<typeAlias type="com.greenart.bdproject.dto.VolunteerApplicationDto" alias="VolunteerApplicationDto"/>
		<typeAlias type="com.greenart.bdproject.dto.VolunteerReviewDto" alias="VolunteerReviewDto"/>
		<typeAlias type="com.greenart.bdproject.dto.MemberStatusHistoryDto" alias="MemberStatusHistoryDto"/>

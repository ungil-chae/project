<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
		PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<!-- 기존 DTO -->
		<typeAlias type="com.greenart.bdproject.dto.BoardDto" alias="BoardDto"/>
		<typeAlias type="com.greenart.bdproject.dto.Member" alias="Member"/>

		<!-- 새로 추가된 DTO (UserDto는 Member로 대체) -->
